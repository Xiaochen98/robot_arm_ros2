<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="DOFBOT_Pro-V24">
    <xacro:arg name="initial_positions_file" default="initial_positions.yaml" />

    <!-- Import DOFBOT_Pro-V24 urdf file -->
    <xacro:include filename="$(find dofbot_pro_description)/urdf/DOFBOT_Pro-V24.urdf" />

    <!-- Import control_xacro -->
    <xacro:include filename="DOFBOT_Pro-V24.ros2_control.xacro" />


    <xacro:DOFBOT_Pro-V24_ros2_control name="FakeSystem" initial_positions_file="$(arg initial_positions_file)"/>

</robot>
